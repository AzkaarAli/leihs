
<% tabnav :item_admin do %>

	<% form_for [:admin, @item] do |f| %>
		<%= f.hidden_field :step %>
	
		<ul>
		<li><b>Inventory Code:</b> <%= f.text_field :inventory_code %></li>	
		<li><b>Serial Number:</b> <%= f.text_field :serial_number %></li>
		</ul>
	
		<%= submit_tag 'Submit' %>
	<% end %>

<% end %>