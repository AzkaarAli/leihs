
<!-- start iterator -->
<%
last_date = Date.new
for @line in @contract.lines  
	if @line.start_date > last_date
  		last_date = @line.start_date
    	%>
		<tr class="line_header"><td colspan="5" class="title"><%= short_date(@line.start_date) %></td></tr>
	<% end %>
	<%= render :partial => @line.type.underscore %>
<% end %>
<!-- end iterator -->						
