		<table cellspacing="0">


		<% for contract in @signed_contracts %>
			<tr>
				<td>
					<%= short_date(contract.lines.min.start_date) %>
				</td>
				<td>
					<%= greybox_link_to_page(contract.user.login,
						 { :controller => 'users', :action => 'show', :id => contract.user.id },
						 :title => _("User"),
						 :class => "iconized-notxt edit-user" ) %>
				</td>
				<td><%
					n = 0
					contract.lines.each { |cl| n += cl.quantity }				
					%>
					<%= _("%{n} Items") % {:n => n} %></td>
					<td>
						<div class="buttons">
						<%= link_to image_tag($layout_public_path + "/images/icons/tab_go.png") + _("Hand Over"), :action => 'show', :id => contract.user_id %>
						</div>
					</td>
			</tr>
		<% end %>

		
		</table>
		