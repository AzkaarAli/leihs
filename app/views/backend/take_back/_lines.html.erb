		<table cellspacing="0">


		<% for line in @grouped_lines %>
			<tr>
				<td>
					<%= short_date(line.end_date) %>
				</td>
				<td>
					<%= greybox_link_to_page(line.user_login,
						 { :controller => 'users', :action => 'show', :id => line.user_id },
						 :title => _("User"),
						 :class => "iconized-notxt edit-user" ) %>
				</td>
				<td><%= _("%{n} Items") % {:n => line.quantity} %></td>
					<td>
						<div class="buttons">
						<%= link_to image_tag($layout_public_path + "/images/icons/tab_go.png") + _("Take Back"), :action => 'show', :id => line.user_id %>
						</div>
					</td>
			</tr>
		<% end %>

		
		</table>
		